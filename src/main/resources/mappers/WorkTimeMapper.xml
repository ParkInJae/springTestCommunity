<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springCommunity.mapper.WorkTimeMapper">

<insert id="CheckIn" parameterType="DailyWorkTimeVO">
 insert into DAILY_WORK_TIME(USER_ID,WORK_DATE,CHECK_IN_TIME,LATITUDE,LONGITUDE)
 values(
	 ${user_id},
	 ${work_date},
	 ${check_in_time},
	 ${latitude},
	 ${longitude}
 )
</insert>



</mapper>